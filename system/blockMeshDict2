FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      blockMeshDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

convertToMeters 0.001;//scale   0.001;

h 26.7;
z 60;
step -16.02;  //H


vertices
(
    (0 0 0)
    (calc "2.5*$h" 0 0)
    (calc "2.5*$h" $step 0)
    (calc "(2.5+35)*$h" $step 0)
    (calc "(2.5+35)*$h" 0 0)
    (calc "(2.5+35)*$h" $h 0)
    (calc "2.5*$h" $h 0)
    (0 $h 0)

    (0 0 $zw)
    (calc "2.5*$h" 0 $zw)
    (calc "2.5*$h" $step $zw)
    (calc "(2.5+35)*$h" $step $zw)
    (calc "(2.5+35)*$h" 0 $zw)
    (calc "(2.5+35)*$h" $h $zw)
    (calc "2.5*$h" $h $zw)
    (0 $h $zw)
);

blocks
(
    hex (0 1 6 7 8 9 14 15) (100 10 1) simpleGrading (1 1 1)
    hex (1 4 5 6 9 12 13 14) (100 10 1) simpleGrading (1 1 1)
    hex (1 2 3 4 9 10 11 12) (100 10 1) simpleGrading (1 1 1)
);

edges
(
);

boundary
(
    // movingWall
    // {
    //     type wall;
    //     faces
    //     (
    //         (3 7 6 2)
    //     );
    // }
    // fixedWalls
    // {
    //     type wall;
    //     faces
    //     (
    //         (0 4 7 3)
    //         (2 6 5 1)
    //         (1 5 4 0)
    //     );
    // }
    // frontAndBack
    // {
    //     type empty;
    //     faces
    //     (
    //         (0 3 2 1)
    //         (4 5 6 7)
    //     );
    // }
    frontAndBack
    {
        type empty;
        faces
        (
            (0 1 8 9)
            (10 11 18 19)

            (1 4 7 8)
            (11 14 17 18)

            (4 5 6 7)
            (14 15 16 17)

            (1 2 3 4) //l f
            (11 12 13 14) //l b
        );
    }
    inlet
    {
        type patch;
        faces
        (
            (0 9 19 10)
        );
    }
    outlet 
    {
        type patch;
        faces 
        (
            (5 6 16 15)
        );
    }
    walls
    {
        type wall;
        faces 
        (

            // (6 7 15 14)
            // (0 8 9 1)        //face 1 in patch 3 does not have neighbour cell face: 4(0 1 9 8)

            // (1 2 10 9)
            // (2 3 11 10)
            // (3 4 12 11)
            // //(4 5 13 12)

            (0 1 11 10)
            (1 2 12 11)
            (2 3 13 12)
            (3 4 14 13)
            (4 5 15 14)

            (9 8 18 19)
            (8 7 17 18)
            (7 6 16 17)
            
            
        );
    }

);

mergePatchPairs
(
);

