//My mesh//


/*--------------------------------*- C++ -*----------------------------------*\
  =========                 |
  \\      /  F ield         | OpenFOAM: The Open Source CFD Toolbox
   \\    /   O peration     | Website:  https://openfoam.org
    \\  /    A nd           | Version:  8
     \\/     M anipulation  |
\*---------------------------------------------------------------------------*/
FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      blockMeshDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

convertToMeters 0.001;

h 26.7;
zw 60;
step_size 16.02;
xtra_x -100;

vertices
(
    // (-7.5 -75   0)
    // ( 7.5 -75   0)
    // ( 7.5  75   0)
    // (-7.5  75   0)
    // (-7.5 -75 450)
    // ( 7.5 -75 450)
    // ( 7.5  75 450)
    // (-7.5  75 450)
    
    ($xtra_x 0 $zw)  //0
    (#calc "2.5*$h" 0 $zw) //1
    (#calc "2.5*$h" $step_size $zw) //2
    (#calc "(2.5+35)*$h" $step_size $zw) //3
    ($xtra_x $h $zw)  //4
    (#calc "2.5*$h" $h $zw) //5
    (#calc "(2.5+35)*$h" $h $zw) //6
    (#calc "(2.5+35)*$h" 0 $zw) //7
    
    
    ($xtra_x 0 0)  //0
    (#calc "2.5*$h" 0 0) //1
    (#calc "2.5*$h" $step_size 0) //2
    (#calc "(2.5+35)*$h" $step_size 0) //3
    ($xtra_x $h 0)  //4
    (#calc "2.5*$h" $h 0) //5
    (#calc "(2.5+35)*$h" $h 0) //6
    (#calc "(2.5+35)*$h" 0 0) //7
);

blocks
(
    // hex (0 1 2 3 4 5 6 7) (1 15 45) simpleGrading (1 1 1)
    hex (8 9 13 12 0 1 5 4) (10 10 1) simpleGrading (1 1 1)
    hex (1 2 3 7 9 10 11 15) (10 10 1) simpleGrading (1 1 1)
    hex (9 15 14 13 1 7 6 5) (10 10 1) simpleGrading (1 1 1) 
);

edges
(
);

boundary
(
    top
    {
        type wall;
        faces
        (
            (4 5 13 12)
            (5 6 14 13)
    
        );
    }

    bottom
    {
        type wall;
        faces
        (
            (0 1 9 8)
            (2 3 11 10)
        );
    }

    step
    {
        type wall;
        faces
        (
            (1 2 10 9)      // err 1 2 8 9
        );
    }

    inlet 
    {
        type patch;
        faces
        (
            (0 8 12 4)
        );
    }
    
    outlet
    {
        type patch;
        faces
        (                               // err ill defined primititive boundary
            (3 11 15 7)
            (7 15 14 6)
        );
    }
    
    frontAndBack
    {
        type empty;
        faces
        (
            (0 1 4 5)
            (2 3 7 1)
            (1 7 6 5)
            
            (8 9 13 12)
            (10 11 15 9)                 
            (9 15 14 13)
        );
    }
);

// ************************************************************************* //




